(ind Nat
    (Z :: Nat)
    (S :: Nat -> Nat))

(add (x y) match x
    (S x' -> S (add x' y))
    (Z -> y))

(pred y match y
    (S y' -> y')
    (Z -> y))

(sub (x y) match x
    (S x' -> pred (sub x' y))
    (Z -> y))

(putnat x match x
    (S x -> (lam (a b) b) (putchr 's') (putnat x))
    (Z -> putchr 'z'))

(let (one S Z))
(let (two S one))
(let (three S two))

(let (test putnat (add three three)))